#!/usr/bin/env bash                                                                      
#Write a Bash script that displays the time for 12 hours and 59 minutes:                 
hours=0                                                                                  
minutes=0                                                                                
while [ $hours -le 12 ]                                                                  
do                                                                                       
  while [ $minutes -le 59 ]                                                              
  do                                                                                     
    echo "$hours:$minutes"                                                               
    ((minutes++))                                                                        
  done                                                                                   
  minutes=0                                                                              
  ((hours++))                                                                            
done | head -70   
