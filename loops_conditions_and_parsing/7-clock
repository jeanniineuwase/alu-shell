#!/usr/bin/env bash                                                                                          
# loop through hours from 0 to 12
for hour in $(seq 0 12)
do
  echo "Hour: $hour"
  # loop through minutes from 0 to 59
  minute=0
  while [ $minute -lt 60 ]
  do
    echo $minute
    minute=$((minute + 1))
  done
done
