#!/usr/bin/env bash                                                                                          
# Initialize the hours and minutes variables                                                                 
hour=0                                                                                                       
minute=1
while [ $hours -lt 12 ]
do
  while [ $minutes -lt 59 ]
  do
    echo "$hours:$minutes"
    minutes=$((minutes + 1))
  done
  hours=$((hours + 1))
  minutes=0
done | head -70
